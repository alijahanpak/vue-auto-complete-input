{
  "name": "vue-auto-complete-input",
  "version": "0.1.7",
  "private": false,
  "description": "Flexible and simple use Vue auto complete input",
  "author": "Ali Jahanpak - Morteza Amzajerdi",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build-bundle": "vue-cli-service build --target lib --name vue-auto-complete-input ./src/components/index.js"
  },
  "main": "./dist/vue-auto-complete-input.common.js",
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "dependencies": {
    "core-js": "^2.6.5",
    "sass-loader": "^7.2.0",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.10.0",
    "@vue/cli-plugin-eslint": "^3.10.0",
    "@vue/cli-service": "^3.10.0",
    "babel-eslint": "^10.0.1",
    "css-loader": "^3.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.12.0",
    "style-loader": "^1.0.0",
    "vue-loader": "^15.7.1",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "bugs": {
    "url": "https://github.com/alijahanpak/vue-auto-complete-input/issues"
  },
  "homepage": "https://github.com/alijahanpak/vue-auto-complete-input#readme",
  "keywords": [
    "Vuejs",
    "vue",
    "input",
    "autocomplete",
    "suggestion",
    "textsuggestion",
    "text",
    "typeahead"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alijahanpak/vue-auto-complete-input.git"
  }
}
